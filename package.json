{
  "name": "quicktextoverride",
  "description": "Use a key command to target the first text override of a symbol.",
  "version": "2.0.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "QuickTextOverride",
    "manifest": "src/manifest.json",
    "main": "quicktextoverride.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "Dmitri Bilyk <dmitri.bilyk@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbilyk/QuickTextOverride"
  },
  "dependencies": {
    "cocoascript-class": "^0.1.2",
    "sketch-module-web-view": "^3.0.7"
  }
}
